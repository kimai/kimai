            <trans-unit id="access.denied">
                <source>access.denied</source>
                <target>Pääsy estetty</target>
            </trans-unit>
            <trans-unit id="stacktrace">
                <source>stacktrace</source>
                <target>Ongelma ilmeni</target>
            </trans-unit>
            <trans-unit id="http_error.title">
                <source>http_error.title</source>
                <target>Virhe</target>
            </trans-unit>
            <trans-unit id="http_error.description">
                <source>http_error.description</source>
                <target>Jotain on pielessä</target>
            </trans-unit>
            <trans-unit id="http_error.suggestion">
                <source>http_error.suggestion</source>
                <target>
                    Kriittinen virhe tapahtui. Yritä uudelleen.
                    Olet mahdollisesti löytänyt ohjelmointivirheen. Ota yhteyttä ylläpitoon jos ongelma jatkuu.
                </target>
            </trans-unit>
