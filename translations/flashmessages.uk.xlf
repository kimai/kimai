      </trans-unit>
      <trans-unit id="xeu1LSy" resname="action.update.error">
        <source>action.update.error</source>
        <target state="translated">Зміни зберегти не вдалося: %reason%</target>
      </trans-unit>
      <trans-unit id="YV50HDB" resname="action.delete.success">
