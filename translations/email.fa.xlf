        <target state="translated">وارد حساب کاربری خود شوید</target>
      </trans-unit>
      <trans-unit id="qbt.84Y" resname="reset.button" xml:space="preserve">
        <source>reset.button</source>
        <target state="translated">وارد شوید</target>
      </trans-unit>
